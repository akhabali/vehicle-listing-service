{
  "openapi": "3.0.1",
  "info": {
    "title": "Vehicle Listing Service",
    "description": "A RESTful service for managing listings for online advertising service.",
    "contact": {
      "name": "Anas Khabali",
      "url": "https://github.com/akhabali",
      "email": "anas.khabali@gmail.com"
    },
    "license": {
      "name": "MIT",
      "url": "https://github.com/akhabali/vehicle-listing-service/blob/main/LICENSE.txt"
    },
    "version": "0.0.1-SNAPSHOT"
  },
  "servers": [
    {
      "url": "http://localhost:8080",
      "description": "Generated server url"
    }
  ],
  "tags": [
    {
      "name": "Listing",
      "description": "Dealer Car Listing Service"
    },
    {
      "name": "Dealer",
      "description": "Dealer Service"
    }
  ],
  "paths": {
    "/dealers": {
      "get": {
        "tags": [
          "Dealer"
        ],
        "summary": "Look up for all dealers",
        "operationId": "findAll",
        "responses": {
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DealerListDto"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Dealer"
        ],
        "summary": "Create a new Dealer",
        "operationId": "createDealer",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/DealerDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DealerDto"
                }
              }
            }
          }
        }
      }
    },
    "/dealers/{dealer_id}/listing": {
      "get": {
        "tags": [
          "Listing"
        ],
        "summary": "Get all vehicle listings of a dealer with a given state",
        "operationId": "findListingByDealerId",
        "parameters": [
          {
            "name": "dealer_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          },
          {
            "name": "state",
            "in": "query",
            "required": true,
            "schema": {
              "type": "string",
              "enum": [
                "draft",
                "published"
              ]
            }
          }
        ],
        "responses": {
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DealerListingDto"
                }
              }
            }
          }
        }
      },
      "post": {
        "tags": [
          "Listing"
        ],
        "summary": "Create a new vehicle listing for a dealer",
        "operationId": "newListing",
        "parameters": [
          {
            "name": "dealer_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CreateListingDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetListingDto"
                }
              }
            }
          }
        }
      },
      "patch": {
        "tags": [
          "Listing"
        ],
        "summary": "Update a vehicle listing of a dealer",
        "operationId": "updateListing",
        "parameters": [
          {
            "name": "dealer_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/UpdateListingDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/GetListingDto"
                }
              }
            }
          }
        }
      }
    },
    "/dealers/{id}": {
      "get": {
        "tags": [
          "Dealer"
        ],
        "summary": "Look up a dealer by its ID",
        "operationId": "findById",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/DealerDto"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "Dealer"
        ],
        "summary": "Delete dealer with 'id'",
        "operationId": "delete",
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int64"
            }
          }
        ],
        "responses": {
          "404": {
            "description": "Not Found",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "409": {
            "description": "Conflict",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ErrorDto"
                }
              }
            }
          },
          "204": {
            "description": "No Content"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ErrorDto": {
        "type": "object",
        "properties": {
          "message": {
            "type": "string"
          }
        }
      },
      "DealerDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "name": {
            "type": "string"
          },
          "tierLimit": {
            "type": "integer",
            "format": "int64"
          }
        }
      },
      "CreateListingDto": {
        "type": "object",
        "properties": {
          "vehicle": {
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          }
        }
      },
      "GetListingDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "dealerId": {
            "type": "integer",
            "format": "int64"
          },
          "vehicle": {
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          },
          "state": {
            "type": "string",
            "enum": [
              "draft",
              "published"
            ]
          }
        }
      },
      "UpdateListingDto": {
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int64"
          },
          "vehicle": {
            "type": "string"
          },
          "price": {
            "type": "number",
            "format": "double"
          }
        }
      },
      "DealerListDto": {
        "type": "object",
        "properties": {
          "dealers": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/DealerDto"
            }
          }
        }
      },
      "DealerListingDto": {
        "type": "object",
        "properties": {
          "listings": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/GetListingDto"
            }
          }
        }
      }
    }
  }
}